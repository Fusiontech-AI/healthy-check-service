<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.fxkc.peis.mapper.TjTeamInfoMapper">

    <select id="queryListByIds" resultType="org.fxkc.peis.domain.vo.TjTeamInfoVo">
        select * from TJ_TEAM_INFO where ID in
        <foreach collection="teamIdList" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>

</mapper>
