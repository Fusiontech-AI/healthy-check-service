<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.fxkc.peis.mapper.TjTemplateExtendMapper">

    <select id="getListByInfoId" resultType="org.fxkc.peis.domain.vo.template.TjTemplateExtendVo">
        select * from TJ_TEMPLATE_EXTEND where TEMPLATE_ID  in
        <foreach collection="templateIdList" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </select>
</mapper>
